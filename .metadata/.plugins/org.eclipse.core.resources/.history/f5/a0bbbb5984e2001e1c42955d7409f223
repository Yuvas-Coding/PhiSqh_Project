package my.com.cmg.iwp.maintenance.model;

import java.util.Date;

public class SearchCriteria {
	private String itemCode;
	private String itemDesc;
	private String epoNo;
	private String lpoNo;
	private String itemStatus;
	private Date lpoAppDateFrom;
	private Date lpoAppDateTo;
	private String customerName;
	private String customerId;
	private String dateFrom;
	private String dateTo;
	private String orderSts;

	public String getItemCode() {
		return itemCode;
	}

	public void setItemCode(String itemCode) {
		this.itemCode = itemCode;
	}

	public String getItemDesc() {
		return itemDesc;
	}

	public void setItemDesc(String itemDesc) {
		this.itemDesc = itemDesc;
	}

	public String getEpoNo() {
		return epoNo;
	}

	public void setEpoNo(String epoNo) {
		this.epoNo = epoNo;
	}

	public String getLpoNo() {
		return lpoNo;
	}

	public void setLpoNo(String lpoNo) {
		this.lpoNo = lpoNo;
	}

	public String getItemStatus() {
		return itemStatus;
	}

	public void setItemStatus(String itemStatus) {
		this.itemStatus = itemStatus;
	}

	public Date getLpoAppDateFrom() {
		return lpoAppDateFrom;
	}

	public void setLpoAppDateFrom(Date lpoAppDateFrom) {
		this.lpoAppDateFrom = lpoAppDateFrom;
	}

	public Date getLpoAppDateTo() {
		return lpoAppDateTo;
	}

	public void setLpoAppDateTo(Date lpoAppDateTo) {
		this.lpoAppDateTo = lpoAppDateTo;
	}

	public String getCustomerName() {
		return customerName;
	}

	public void setCustomerName(String customerName) {
		this.customerName = customerName;
	}

	public String getCustomerId() {
		return customerId;
	}

	public void setCustomerId(String customerId) {
		this.customerId = customerId;
	}

	public String getDateFrom() {
		return dateFrom;
	}

	public void setDateFrom(String dateFrom) {
		this.dateFrom = dateFrom;
	}

	public String getDateTo() {
		return dateTo;
	}

	public void setDateTo(String dateTo) {
		this.dateTo = dateTo;
	}

	public String getOrderSts() {
		return orderSts;
	}

	public void setOrderSts(String orderSts) {
		this.orderSts = orderSts;
	}
}

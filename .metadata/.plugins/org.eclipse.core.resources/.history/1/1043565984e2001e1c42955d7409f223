package my.com.cmg.iwp.maintenance.model;

import java.math.BigDecimal;
import java.util.Date;

public class IntegrationBean {
	private Date integrationDate;
	private String integrationType;
	private String status;
	private BigDecimal noOfTransactions;
	private String intType;
	private String facilityName;
	private String facilityCode;
	
	public Date getIntegrationDate() {
		return integrationDate;
	}
	public void setIntegrationDate(Date integrationDate) {
		this.integrationDate = integrationDate;
	}
	public String getIntegrationType() {
		return String.valueOf(integrationType);
	}
	public void setIntegrationType(String integrationType) {
		this.integrationType = integrationType;
	}
	public String getStatus() {
		return String.valueOf(status);
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public Integer getNoOfTransactions() {
		return noOfTransactions.intValue();
	}
	public void setNoOfTransactions(BigDecimal noOfTransactions) {
		this.noOfTransactions = noOfTransactions;
	}
	public String getIntType() {
		return intType;
	}
	public void setIntType(String intType) {
		this.intType = intType;
	}
	
	public String getFacilityName() {
		return facilityName;
	}
	public void setFacilityName(String facilityName) {
		this.facilityName = facilityName;
	}
	public String getFacilityCode() {
		return facilityCode;
	}
	public void setFacilityCode(String facilityCode) {
		this.facilityCode = facilityCode;
	}	
}
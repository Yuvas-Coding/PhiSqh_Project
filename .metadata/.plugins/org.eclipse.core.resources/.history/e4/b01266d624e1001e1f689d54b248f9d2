package my.com.cmg.iwp.maintenance.service;

import java.util.List;

import my.com.cmg.iwp.maintenance.model.Manufacturer;

public interface ManufacturerService {

	Manufacturer getNewManufacturer();

	void saveOrUpdate(Manufacturer anManufacturer);

	void delete(Manufacturer anManufacturer);

	public List<Manufacturer> getAllManufacturers();

	public Manufacturer findByManufacturerCode(String manufacturerCode);

	String getManufacturerNo(Manufacturer anManufacturer);

	Manufacturer findByManufacturerName(String value);

	Manufacturer getManufacturerBySeqNo(Long manuSeqno);

}

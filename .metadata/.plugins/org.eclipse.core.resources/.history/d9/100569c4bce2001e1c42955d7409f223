	@Column(name = "drug_type")
	public String getDrugType() {
		return drugType;
	}

	public void setDrugType(String drugType) {
		this.drugType = drugType;
	}

	@OneToMany(fetch = FetchType.LAZY, mappedBy = "drug", cascade = CascadeType.ALL)
	@Cascade({org.hibernate.annotations.CascadeType.SAVE_UPDATE, org.hibernate.annotations.CascadeType.DELETE_ORPHAN})
	public Set<DrugIndication> getDrugIndications() {
		return drugIndications;
	}

	public void setDrugIndications(Set<DrugIndication> drugIndications) {
		this.drugIndications = drugIndications;
	}


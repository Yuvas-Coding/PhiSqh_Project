package my.com.cmg.iwp.maintenance.service;

import java.util.List;

import my.com.cmg.iwp.maintenance.model.Discipline;

public interface DisciplineService {

	void delete(Discipline anDiscipline);

	Discipline getNewDiscipline();

	void saveOrUpdate(Discipline anDiscipline);

	List<Discipline> findByDisciplineCode(String disciplineCode);

	List<Discipline> getAllDiscipline();

	Discipline getDisciplineBySeqNo(Long disciplineSeqno);

	List<Discipline> getAllActiveDiscipline();

}